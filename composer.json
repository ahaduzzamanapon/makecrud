    "keywords": [
        "laravel",
        "crud",
        "rest",
        "generator",
        "rest-api",
        "laravel-package",
        "crud-generator",
        "laravel-crud-generator",
        "composer-package",
        "laravel-crud",
        "laravel8",
        "laravel9",
        "laravel10"
